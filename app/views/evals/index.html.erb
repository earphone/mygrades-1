<h1>Listing evals</h1>

<table>
  <tr>
    <th>Rating</th>
    <th>Task</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @evals.each do |eval| %>
  <tr>
    <td><%= eval.rating %></td>
    <td><%= eval.Task %></td>
    <td><%= link_to 'Show', eval %></td>
    <td><%= link_to 'Edit', edit_eval_path(eval) %></td>
    <td><%= link_to 'Destroy', eval, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Eval', new_eval_path %>
